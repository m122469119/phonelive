<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/graywhite">

    <com.zhy.autolayout.AutoLinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">


        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/rl_change_pass"
            style="@style/option_item_rl"
            android:layout_marginTop="@dimen/space_15"
            android:background="@color/white">

            <TextView
                style="@style/option_item_text"
                android:text="修改密码" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:src="@drawable/me_next" />
        </com.zhy.autolayout.AutoRelativeLayout>

        <View style="@style/h_line" />

        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/ll_push_manage"
            style="@style/option_item_rl"
            android:background="@color/white">

            <TextView
                style="@style/option_item_text"
                android:text="@string/pushmanage" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:src="@drawable/me_next" />
        </com.zhy.autolayout.AutoRelativeLayout>

        <View style="@style/h_line" />

        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/ll_about"
            style="@style/option_item_rl"
            android:background="@color/white">

            <TextView
                style="@style/option_item_text"
                android:text="@string/about" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:src="@drawable/me_next" />
        </com.zhy.autolayout.AutoRelativeLayout>

        <View style="@style/h_line" />

        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/ll_feedback"
            style="@style/option_item_rl"
            android:background="@color/white">

            <TextView
                style="@style/option_item_text"
                android:text="@string/feedback" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:src="@drawable/me_next" />
        </com.zhy.autolayout.AutoRelativeLayout>

        <View style="@style/h_line" />

        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/ll_clearCache"
            style="@style/option_item_rl"
            android:background="@color/white">

            <TextView
                style="@style/option_item_text"
                android:text="清理缓存" />

        </com.zhy.autolayout.AutoRelativeLayout>

        <View style="@style/h_line" />

        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/ll_blank_list"
            style="@style/option_item_rl"
            android:layout_marginTop="@dimen/space_10"
            android:background="@color/white">

            <TextView
                style="@style/option_item_text"
                android:text="@string/blanklist" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:src="@drawable/me_next" />
        </com.zhy.autolayout.AutoRelativeLayout>

        <View style="@style/h_line" />

        <com.zhy.autolayout.AutoRelativeLayout
            android:id="@+id/ll_check_update"
            style="@style/option_item_rl"
            android:layout_marginTop="@dimen/space_10"
            android:background="@color/white">

            <TextView
                style="@style/option_item_text"
                android:text="检查更新" />

        </com.zhy.autolayout.AutoRelativeLayout>

        <com.zhy.autolayout.AutoLinearLayout
            android:id="@+id/ll_loginout"
            style="@style/option_item_rl"
            android:layout_marginTop="@dimen/space_15"
            android:background="@color/white">

            <TextView
                style="@style/option_item_text"
                android:layout_width="fill_parent"
                android:gravity="center"
                android:text="@string/loginout" />
        </com.zhy.autolayout.AutoLinearLayout>
    </com.zhy.autolayout.AutoLinearLayout>

    <View
        android:id="@+id/bg_gone"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/bg_black"/>
</com.zhy.autolayout.AutoFrameLayout>